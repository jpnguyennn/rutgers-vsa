// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BoardMember {
  id            Int     @id @default(autoincrement())
  positional_id Int
  full_name     String?
  position      String?
  photo_url     String?
  facebook      String?
  instagram     String?
  vsa_email     String?
  year          Int
  major         String?
  why_vsa       String?
  slug          String?
}

model Intern {
  id        Int     @id @default(autoincrement())
  full_name String?
  photo_url String?
  facebook  String?
  instagram String?
  year      Int
  major     String?
}
